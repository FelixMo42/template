{
    "name": "pixi-svelte-template",
    "version": "0.0.0",
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c --watch",
        "watch": "nodemon server/server.js",
        "start": "node server/server.js"
    },
    "nodemonConfig": {
        "ignore": [
            "client/*",
            "data/*"
        ]
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^12.0.0",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "rollup": "^2.3.4",
        "rollup-plugin-livereload": "^1.0.0",
        "rollup-plugin-svelte": "^5.0.3",
        "rollup-plugin-terser": "^5.1.2",
        "svelte": "^3.0.0"
    },
    "dependencies": {
        "express": "^4.17.1",
        "express-middleware-rollup": "^3.0.0",
        "nodemon": "^2.0.4",
        "pixi.js": "^5.3.0"
    }
}
